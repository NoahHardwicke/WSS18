  FormBox[
   RowBox[{"x", 
    SuperscriptBox[".", "5"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ea08b9c5-fefd-4019-8fae-bda895100ab2"]
