              SuperscriptBox["c", "2"]}]}], ")"}], "2"]}], "\[Equal]", 
         RowBox[{
          FractionBox[
           RowBox[{"q", " ", "c"}], 
           RowBox[{"6", " ", "\[Pi]", " ", "\[Epsilon]0"}]], "*", 
          SuperscriptBox[
           RowBox[{
            RowBox[{"v", "'"}], "[", "t", "]"}], "2"]}]}], ",", 
        RowBox[{
         RowBox[{"v", "[", "0", "]"}], "\[Equal]", "v0"}]}], "}"}], ",", 
      RowBox[{"v", "[", "t", "]"}], ",", "t"}], "]"}], "\[LeftDoubleBracket]",
